  "engines": {
    "node": ">=22.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"db/schema/**/*.ts\"",
    "studio": "drizzle-kit studio",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "migrate.dev": "pnpm drizzle-kit generate && pnpm drizzle-kit migrate",
    "prestart:debug": "pnpm migrate.dev",
    "prestart:dev": "pnpm migrate.dev",
    "migrate.prod": "pnpm drizzle-kit migrate",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "drop": "pnpm drizzle-kit drop",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db.seed": "pnpm tsx scripts/seed.ts",
    "tunnel": "pnpm tsx scripts/tunnel/index.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.4",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.0.2",
    "@scalar/nestjs-api-reference": "^0.3.177",
    "axios": "^1.7.9",
    "drizzle-orm": "^0.38.4",
    "drizzle-zod": "^0.6.1",
    "google-libphonenumber": "^3.2.40",
    "lru-cache": "^11.0.2",
    "nestjs-zod": "^4.2.0",
    "ngrok": "5.0.0-beta.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "telegraf": "^4.16.3",
    "zod": "^3.24.1",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
